{
  "id": "nodebb-plugin-composer-latex",
  "name": "Composer LaTeX",
  "description": "Adds a LaTeX button to the composer toolbar and MathJax rendering for math equations",
  "url": "https://github.com/nodebb/nodebb-plugin-composer-latex",
  "library": "./library.js",
  "scripts": [
    "public/js/client.js",
    "public/js/mathjax.js"
  ],
  "languages": "public/languages",
  "hooks": [
    { "hook": "filter:composer.formatting", "method": "registerFormatting" },
    { "hook": "filter:middleware.renderHeader", "method": "addMathJaxScript" }
  ]
}
